<%= image_tag @user.photo %>
<h1><%= @user.name %></h1>
<p>Bio: <%= @user.bio %></p>
<% @posts.each do |post| %>
  <div>
    <h3><%= link_to post.title, user_post_path(@user, post) %></h3>
    <p><%= post.text %></p>
    <p>Likes: <%= post.likes_counter %></p>
    <p>Comments: <%= post.comments_counter %></p>
  </div>
<% end %>
